#!/bin/bash

echo "Hello world"

make seq omp2
cd exe/

for k in 2 4 8 16 32; do
    echo "K = $k"
    ./k-means_seq $k random_data_40000_200
     export OMP_NUM_THREADS=2
    ./k-means_omp_v2 $k random_data_40000_200
    export OMP_NUM_THREADS=4
    ./k-means_omp_v2 $k random_data_40000_200
     export OMP_NUM_THREADS=8
    ./k-means_omp_v2 $k random_data_40000_200
done


